-- teamwork:
--   game that sarah and abbey
--   can play together
--   to practice teamwork
--   in a slightly fun way

function _init()
	level=1
	starttitle()
end

function starttitle()
	progress = 0
	_draw=drawtitle
	_update=updatetitle
end

function drawtitle()
	cls(1)
	print("level "..level,50,50,7)
	local w = progress * (80-45)
	line(45,60,80  ,60,0)
	line(45,60,45+w,60,2)
end

function updatetitle()
	progress += (1/30/3) -- 3 sec
	if progress >= 1 then
		startgame()
	end
end

-->8
function startgame()
	_draw=drawgame
	_update=updategame
end

function drawgame()
	cls()
end

function updategame()
end

