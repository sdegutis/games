-- buttons

function makeallbutton()
	return {
		on=false,
		buttons={},
		bricks={},
		boxes={},
	}
end

function updatebuttons()
	for i=1,2 do
		local b=buttons[i]
		local p=players[i]
		
		local anypressed = false
		for i=1,#b.buttons do
			local bb=b.buttons[i]
			if collided(p,bb) then
				anypressed=true
				break
			end
			for i=1,#b.boxes do
				if collided(bb,b.boxes[i]) then
					anypressed=true
					break
				end
			end
		end
		
		if b.on and not anypressed then
			b.on=false
			activate(b)
		elseif not b.on and anypressed then
			b.on=true
			activate(b)
		end
	end
end

function activate(b)
	if b.on then
		sfx(10)
	else
		sfx(11)
	end
	
	for i=1,#b.buttons do
		b.buttons[i].on = b.on
	end
	
	for i=1,#b.bricks do
		b.bricks[i].up = not b.bricks[i].up
	end
end

