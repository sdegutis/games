-- util

function getitem()
 for i = 1, #items do
  local item = items[i]

  local x1 = item.x-4
  local y1 = item.y-4
  local x2 = item.x+4
  local y2 = item.y+4

  if hero.x>=x1 and hero.x<=x2 and
     hero.y>=y1 and hero.y<=y2 then
   return item, i
  end
 end
end

function sprat(x,y)
 local tx = flr((hero.x+x) / 8)
 local ty = flr((hero.y+y) / 8)
 return mget(tx,ty), tx, ty
end

function _sprat(x,y)
 local tx = flr(x / 8)
 local ty = flr(y / 8)
 return mget(tx,ty)
end

function slowarea()
	return slow(sprat(0,0))
	    or slow(sprat(7,0))
	    or slow(sprat(0,7))
	    or slow(sprat(7,7))
end

function notwater()
	return not water(sprat(0,0))
	   and not water(sprat(7,0))
	   and not water(sprat(0,7))
	   and not water(sprat(7,7))
end

function onsign()
	return issign(sprat(3,3))
	    or issign(sprat(4,3))
	    or issign(sprat(3,4))
	    or issign(sprat(4,4))
end

function issign(s, x, y)
	if s == 14 then
		 return {x, y}
 end
end

function slow(s)
 return fget(s,1)
end

function water(s)
	return (s>=26 and s<=31)
	    or (s>=41 and s<=47)
end

function air(s)
	return not fget(s,0)
end

function heart(s)
	return fget(s,2)
end

