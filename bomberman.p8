-- bombs


function makebomb(p)
 local sec=p.qwk
 local x = round(p.x/8)*8
 local y = round(p.y/8)*8
	add(bombs,{
		x=x,
		y=y,
		player=p,
		range=p.pwr,
		t=sec*30,
	})
end

function updatebomb(b)
	b.t -= 1
	if b.t == 0 then
		del(bombs,b)
		b.player.bombs_live-=1
		makeflames(b)
	end
end

function drawbomb(b)
	local s = 7
	if (time()%1 < 0.5) s=8
	spr(s, b.x, b.y)
end

