-- maps


mapmax_x=4
mapmax_y=4

mapx=0
mapy=0

function choosemap()
	_update=updatemapchooser
	_draw=drawmapchooser
end

function drawmapchooser()
	cls()
	for y = 1, mapmax_y do
		for x = 1, mapmax_x do
			local mx = x-1
			local my = y-1
			
			local px = mx*30+5
			local py = my*30+5
			
			local d = round(cos(time()%1)/3*4)+2
			
			if mx==mapx and my==mapy then
				rect(px-1-d, py-1-d,
				     px+24+d,py+24+d,
				     7)
			end
			
			map(mx*16,my*16,px,py,3,3)
			local b=mget(mx*16+15,my*16)
			spr(b,px+8,py+16)
		end
	end
end

function updatemapchooser()
	if btnp(⬆️,0) then
		sfx(2)
		mapy -= 1
		if (mapy<0) mapy = mapmax_y-1
	elseif btnp(⬇️,0) then
		sfx(2)
		mapy += 1
		if (mapy==mapmax_y) mapy=0
	elseif btnp(⬅️,0) then
		sfx(2)
		mapx -= 1
		if (mapx<0) mapx = mapmax_x-1
	elseif btnp(➡️,0) then
		sfx(2)
		mapx += 1
		if (mapx==mapmax_x) mapx=0
	elseif btnp(❎,0) then
		sfx(3)
		mapb=mget((mapx*16)+15,mapy*16)
		startgame()
	end
end

