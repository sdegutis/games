pico-8 cartridge // http://www.pico-8.com
version 33
__lua__


-- note: make them waterbombs
-- that way no one gets hurt.

function _init()
	p1=makeplayer(8,8,16)
	p2=makeplayer(104,104,32)
end

function _draw()
	cls()
	map()
	drawplayer(p1)
	drawplayer(p2)
end

function _update()
	
end

function drawplayer(p)
	spr(p.s,p.x,p.y)
end

function makeplayer(x,y,s)
	return {
		x=x,
		y=y,
		mx=0,
		my=0,
		s=s,
	}
end

