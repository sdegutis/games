-- flames


function makeflames(b)
	addflame(b,0,0,23)
	for x = -1,-b.range,-1 do
		if (addflame(b,x,0,39)) break
	end
	for y = -1,-b.range,-1 do
		if (addflame(b,0,y,55)) break
	end
	for x = 1,b.range do
		if (addflame(b,x,0,39)) break
	end
	for y = 1,b.range do
		if (addflame(b,0,y,55)) break
	end
end

function addflame(b,x,y,s)
	local px = b.x+x*8
	local py = b.y+y*8
	
	if hasstone(px,py) then
		return true
	end
	
	add(flames,{
		x=px,
		y=py,
		t=9,
		s=s,
	})
	
	local brick=getbrick(px,py)
	if brick then
		del(bricks,brick)
		
		if brick.k then
			makeitem(brick)
		end
		
		return true
	end
	
	local item = getitem(px,py)
	if item then
		del(items,item)
	end
	
	local bomb = getbomb(px,py)
	if bomb then
		bomb.t=1
		return true
	end
	
	return false
end

function drawflame(f)
	local adds = flr((9-f.t)/3)
	if (f.s == 9) adds=0
	spr(f.s+adds, f.x, f.y)
	
	f.t -= 1
	if f.t == 0 then
		del(flames, f)
	end
end

function updateflame(f)
	
end

