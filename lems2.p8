-- grid

function outside(x,y)
	return x < 0
	    or y < 0
	    or x > 31
	    or y > 31
end

function setcell(x,y, cell)
	cell.x = x
	cell.y = y
	grid[y][x] = cell
end

function makegrid()
	grid = {}
	for y = 0,31 do
		grid[y] = {}
		for x = 0,31 do
			setcell(x, y, new(air))
		end
	end
end

