-- mouse

function makeplacer()
 placer = new(placers[placers.i])
end

function updatemouse()
 local scrollby = stat(36)
 if scrollby != 0 then
 	placers.i += scrollby
 	if(placers.i<1)placers.i=#placers
 	if(placers.i>#placers)placers.i=1
  makeplacer()
 end
 
 mx=min(31,max(0,flr(stat(32)/4)))
	my=min(28,max(0,flr(stat(33)/4)))
	
	local b = stat(34)
	if band(b, 1) > 0 then
		local x2 = lmx or mx
		local y2 = lmy or my
		local points = makeline(mx,my,x2,y2)
		for p in all(points) do
			setcell(p.x,p.y, placer)
			makeplacer()
		end
		
		lmx = mx
		lmy = my
	else
		lmx = nil
		lmy = nil
	end
	
	if band(b, 2) > 0 then
		delfloors()
	end
end

function delfloors()
	for y=0,31 do
		for x=0,31 do
			local cell = grid[y][x]
			if not cell.movable then
				setcell(x,y, new(air))
			end
		end
	end
end

function makeline(x1,y1,x2,y2)
	local points = {}
	local seen = {}
	
	local diffx = abs(x2-x1)
	local diffy = abs(y2-y1)
	local maxdiff = max(diffx,diffy)
	
	local dirx = sgn(x2-x1)
	local diry = sgn(y2-y1)
	
	for i = 0,maxdiff do
		local x = x1
		local y = y1
		
		if maxdiff > 0 then
			x += flr(i * diffx / maxdiff * dirx)
			y += flr(i * diffy / maxdiff * diry)
		end
		
		local seenkey = x..","..y
		if not seen[seenkey] then
			seen[seenkey] = true
			add(points,{x=x,y=y})
		end
	end
	
	return points
end

