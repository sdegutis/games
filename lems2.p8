-- menu

buttons = {
	"moon",
	"moving",
	"rainlems",
	"many",
	"immortal",
}

function drawmenu()
	rectfill(0,117,127,127,0)
	
 for i = 1,#buttons do
 	local x = 1 + (i-1) * 9
 	local y = 118
 	local s = 0x10+(i-1)
 	local opt = buttons[i]
 	
		rectfill(x,y,x+7,y+7,1)
		if not opts[opt] then
			for j=1,15 do
				pal(j,5)
			end
		end
		spr(s,x,y)
		pal()
 end
end

menulastdown = false

function updatemenu()
 mx=stat(32)
	my=stat(33)
	local down = (stat(34) == 1)
	
	if down and not menulastdown then
		local i = ceil((mx+1)/9)
		if i >= 1 and i <= #buttons then
 		local opt = buttons[i]
 		opts[opt] = not opts[opt]
		end
	end
	
 menulastdown = down
end

